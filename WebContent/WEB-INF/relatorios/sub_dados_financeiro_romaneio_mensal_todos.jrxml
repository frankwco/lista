	<queryString>
		<![CDATA[SELECT gestaofacil.coleta.`dataColeta`,
	gestaofacil.coleta.observacao,
	gestaofacil.pessoa.`nomeRazaoSocial`,
	gestaofacil.pessoa.`nomeFantasia`,
	gestaofacil.coleta.`fechamentoMes_id`,
	gestaofacil.coleta.id
